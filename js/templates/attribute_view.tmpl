  <div class="modal fade" id="attribute_view">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title" id="attributes_view_title"><%= name %></h4>
      </div>
      <div class="modal-body">
        <table id="attributes_table">
          <% _.forEach(attributes, function(attr) { %>
            <tr>
              <td><%=key%></td>
              <td><%=value%></td>
            </tr>
          <%});%>
        </table>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" id="btn_close">Close</button>
        <button type="button" class="btn btn-primary" id="btn_save">Save changes</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
